{
  "name": "sleep-api",
  "version": "2.7.1",
  "private": true,
  "main": "app.ts",
  "type": "module",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "tsc && tsc-alias && tsoa spec",
    "postbuild": "copyfiles -u 1 src/**/*.html src/**/*.css src/**/**/*.json src/**/*.png src/assets/* src/assets/**/* dist/",
    "compile": "bun x tsc --noEmit",
    "test-watch": "bun test --watch",
    "start": "npm run build && node ./dist/app.js",
    "dev": "bun --watch src/app.ts"
  },
  "files": ["dist"],
  "devDependencies": {
    "@types/cors": "^2.8.15",
    "@types/express": "^4.17.20",
    "@types/morgan": "^1.9.7",
    "@types/node": "^20.11.20",
    "@types/swagger-jsdoc": "^6.0.2",
    "@types/swagger-ui-express": "^4.1.5",
    "bun-types": "^1.1.42",
    "bunboozle": "^1.3.1",
    "copyfiles": "^2.4.1",
    "rimraf": "^6.0.1",
    "sleepapi-common": "file:../common",
    "sqlite3": "^5.1.6",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.8",
    "typescript": "^5.7.2"
  },
  "dependencies": {
    "@sinclair/typebox": "^0.31.22",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "google-auth-library": "^9.10.0",
    "knex": "^3.1.0",
    "morgan": "~1.9.1",
    "mysql2": "~3.9.7",
    "swagger-ui-express": "^5.0.0",
    "tsoa": "^6.0.1",
    "workerpool": "^9.2.0"
  },
  "bundleDependencies": ["sleepapi-common"]
}
