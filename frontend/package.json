{
  "name": "sleepapi-frontend",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "clean": "rimraf dist",
    "prebuild": "npm install && npm run clean",
    "build": "run-p type-check build-only",
    "build-only": "vite build",
    "preview": "vite preview",
    "test": "vitest --run --coverage",
    "test-watch": "vitest --changed",
    "test-watch-all": "vitest",
    "type-check": "vue-tsc --build --force",
    "eslint-check": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --ignore-path .gitignore",
    "eslint-fix": "npm run eslint-check -- --fix",
    "prettier-check": "prettier --check .",
    "prettier-fix": "prettier --write .",
    "lint": "npm run eslint-check && npm run prettier-check",
    "lint-fix": "npm run eslint-fix && npm run prettier-fix"
  },
  "dependencies": {
    "@faker-js/faker": "^8.4.1",
    "@mdi/font": "^7.4.47",
    "axios": "^1.6.8",
    "axios-mock-adapter": "^1.22.0",
    "chart.js": "^4.4.4",
    "pinia": "^2.1.7",
    "pinia-plugin-persistedstate": "^3.2.1",
    "vue": "^3.4.21",
    "vue-chartjs": "^5.3.1",
    "vue-router": "^4.3.0",
    "vue3-google-login": "^2.0.26"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.3.3",
    "@tsconfig/node20": "^20.1.2",
    "@types/jsdom": "^21.1.6",
    "@types/node": "^20.11.28",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vitest/coverage-v8": "^1.5.0",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/test-utils": "^2.4.5",
    "@vue/tsconfig": "^0.5.1",
    "eslint": "^8.49.0",
    "eslint-plugin-vue": "^9.17.0",
    "jsdom": "^24.0.0",
    "npm-run-all2": "^6.1.2",
    "prettier": "^3.0.3",
    "resize-observer-polyfill": "^1.5.1",
    "sass": "^1.74.1",
    "sleepapi-common": "file:../common",
    "typescript": "~5.4.0",
    "vite": "^5.1.6",
    "vite-plugin-pwa": "^0.20.0",
    "vite-plugin-vue-devtools": "^7.2.1",
    "vite-plugin-vuetify": "^2.0.3",
    "vitest": "^1.4.0",
    "vue-tsc": "^2.0.6",
    "vuetify": "~3.6.8"
  },
  "bundleDependencies": [
    "sleepapi-common"
  ]
}
